
public class AlphabetsAZforLoop {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		char c;
		for (c = 'A';c <= 'T'; c++) {
			System.out.print(c + ",");
		}
		System.out.println("/n");
		char ch; 
		
		for (c = 'A';c <= 'T'; c++) {
			System.out.print(c + "-");
		}
	}

}
